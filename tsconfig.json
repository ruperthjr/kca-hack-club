{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "es2015",
		"moduleResolution": "bundler"
	},
	"exclude": ["svelte.config.js", ".svelte-kit", "node_modules"]
}